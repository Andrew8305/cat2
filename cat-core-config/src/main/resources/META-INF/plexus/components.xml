<plexus>
	<components>
		<component>
			<role>org.unidal.initialization.Module</role>
			<role-hint>cat-core-config</role-hint>
			<implementation>org.unidal.cat.core.config.CatConfigModule</implementation>
		</component>
		<component>
			<role>org.unidal.cat.core.config.DomainGroupConfigService</role>
			<implementation>org.unidal.cat.core.config.DefaultDomainGroupConfigService</implementation>
		</component>
		<component>
			<role>org.unidal.cat.core.config.DomainOrgConfigService</role>
			<implementation>org.unidal.cat.core.config.DefaultDomainOrgConfigService</implementation>
		</component>
		<component>
			<role>org.unidal.cat.core.config.ConfigProviderManager</role>
			<implementation>org.unidal.cat.core.config.DefaultConfigProviderManager</implementation>
			<requirements>
				<requirement>
					<role>org.unidal.cat.core.config.dal.ReportConfigDao</role>
				</requirement>
			</requirements>
		</component>
		<component>
			<role>org.unidal.dal.jdbc.mapping.TableProvider</role>
			<role-hint>report-config</role-hint>
			<implementation>org.unidal.dal.jdbc.mapping.SimpleTableProvider</implementation>
			<configuration>
				<physical-table-name>report_config</physical-table-name>
				<data-source-name>cat</data-source-name>
			</configuration>
		</component>
		<component>
			<role>org.unidal.cat.core.config.dal.ReportConfigDao</role>
			<implementation>org.unidal.cat.core.config.dal.ReportConfigDao</implementation>
			<requirements>
				<requirement>
					<role>org.unidal.dal.jdbc.QueryEngine</role>
				</requirement>
			</requirements>
		</component>
	</components>
</plexus>
