<plexus>
	<components>
		<component>
			<role>org.unidal.initialization.Module</role>
			<role-hint>cat-core-config</role-hint>
			<implementation>org.unidal.cat.core.config.CatConfigModule</implementation>
		</component>
		<component>
			<role>org.unidal.cat.core.config.service.DomainGroupConfigService</role>
			<implementation>org.unidal.cat.core.config.service.DefaultDomainGroupConfigService</implementation>
		</component>
		<component>
			<role>org.unidal.cat.core.config.service.DomainOrgConfigService</role>
			<implementation>org.unidal.cat.core.config.service.DefaultDomainOrgConfigService</implementation>
		</component>
		<component>
			<role>org.unidal.cat.core.config.ConfigProviderManager</role>
			<implementation>org.unidal.cat.core.config.DefaultConfigProviderManager</implementation>
			<requirements>
				<requirement>
					<role>org.unidal.cat.core.config.dal.ReportConfigDao</role>
				</requirement>
			</requirements>
		</component>
		<component>
			<role>org.unidal.dal.jdbc.mapping.TableProvider</role>
			<role-hint>report-config</role-hint>
			<implementation>org.unidal.dal.jdbc.mapping.SimpleTableProvider</implementation>
			<configuration>
				<physical-table-name>report_config</physical-table-name>
				<data-source-name>cat</data-source-name>
			</configuration>
		</component>
		<component>
			<role>org.unidal.cat.core.config.dal.ReportConfigDao</role>
			<implementation>org.unidal.cat.core.config.dal.ReportConfigDao</implementation>
			<requirements>
				<requirement>
					<role>org.unidal.dal.jdbc.QueryEngine</role>
				</requirement>
			</requirements>
		</component>
		<component>
			<role>org.unidal.web.mvc.model.ModuleRegistry</role>
			<implementation>org.unidal.web.mvc.model.ModuleRegistry</implementation>
			<configuration>
				<defaultModule>org.unidal.cat.core.config.system.SystemModule</defaultModule>
			</configuration>
		</component>
		<component>
			<role>org.unidal.web.mvc.Module</role>
			<role-hint>org.unidal.cat.core.config.system.SystemModule</role-hint>
			<implementation>org.unidal.cat.core.config.system.SystemModule</implementation>
		</component>
		<component>
			<role>org.unidal.cat.core.config.system.page.Handler</role>
			<implementation>org.unidal.cat.core.config.system.page.Handler</implementation>
			<requirements>
				<requirement>
					<role>org.unidal.cat.core.config.system.page.JspViewer</role>
				</requirement>
			</requirements>
		</component>
		<component>
			<role>org.unidal.cat.core.config.system.page.JspViewer</role>
			<implementation>org.unidal.cat.core.config.system.page.JspViewer</implementation>
			<requirements>
				<requirement>
					<role>org.unidal.web.mvc.view.model.ModelHandler</role>
				</requirement>
			</requirements>
		</component>
	</components>
</plexus>
