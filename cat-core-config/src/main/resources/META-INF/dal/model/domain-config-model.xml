<?xml version="1.0" encoding="UTF-8"?>
<model model-package="org.unidal.cat.core.config.domain" class-suffix="Model"
  enable-sax-parser="true">
  <entity name="domain-config" root="true">
    <entity-ref name="domain" type="map" names="domains" />
  </entity>
  <entity name="domain">
    <attribute name="id" value-type="String" key="true"/>
    <entity-ref name="machine" type="map" names="machines" />
  </entity>
  <entity name="machine" dynamic-attributes="true">
    <attribute name="ip" value-type="String" key="true" />
    <attribute name="host" value-type="String" />
    <attribute name="group" value-type="String" enabled="false" />
    <attribute name="test" value-type="boolean" enabled="false" />
  </entity>
</model>

